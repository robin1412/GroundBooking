<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.userbooking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/football"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UserBooking">
        <activity android:name=".BookingSlot"
            android:screenOrientation="portrait"
            android:parentActivityName=".UserHome">

        </activity>
        <activity android:name=".DisplaySeller"
            android:screenOrientation="portrait"
            android:parentActivityName=".UserHome"/>
        <activity android:name=".UserHome"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>